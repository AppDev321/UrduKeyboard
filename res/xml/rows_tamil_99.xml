<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <Row latin:keyWidth="8.3%p" >
        <include latin:keyboardLayout="@xml/rowkeys_tamil_99_1" />
    </Row>
    <Row latin:keyWidth="8.1%p" >
        <include
            latin:keyboardLayout="@xml/rowkeys_tamil_99_2"
            latin:keyXPos="5%p" />
    </Row>
    <Row latin:keyWidth="8%p" >
        <Key
            latin:keyStyle="shiftKeyStyle"
            latin:keyWidth="10%p" />
        <include
            latin:keyboardLayout="@xml/rowkeys_tamil_99_3" />
        <Key
            latin:keyStyle="deleteKeyStyle"
            latin:keyWidth="fillRight" />
    </Row>
    <include latin:keyboardLayout="@xml/row_qwerty4" />
</merge>
