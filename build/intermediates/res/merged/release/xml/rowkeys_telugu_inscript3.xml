<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <!--A ఓ TELUGU LETTER OO-->
            <Key
                latin:keySpec="&#x0C13;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--S ఏ TELUGU LETTER EE-->
            <Key
                latin:keySpec="&#x0C0F;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--D అ TELUGU LETTER A-->
            <Key
                latin:keySpec="&#x0C05;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--F ఇ TELUGU LETTER I-->
            <Key
                latin:keySpec="&#x0C07;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--G ఉ TELUGU LETTER U-->
            <Key
                latin:keySpec="&#x0C09;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--H ఫ TELUGU LETTER PHA-->
            <Key
                latin:keySpec="&#x0C2B;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--J ఱ TELUGU LETTER RRA-->
            <Key
                latin:keySpec="&#x0C31;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--K ఖ TELUGU LETTER KHA-->
            <Key
                latin:keySpec="&#x0C16;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--L థ TELUGU LETTER THA-->
            <Key
                latin:keySpec="&#x0C25;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--: &#x0C1B; ఛTELUGU LETTER CHA -->
            <Key
                latin:keySpec="&#x0C1B;"
                latin:moreKeys=":"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--" ఠ TELUGU LETTER TTHA-->
            <Key
                latin:keySpec="&#x0C20;"
                latin:moreKeys="&quot;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
        </case>

        <default>
            <!--a ో TELUGU VOWEL SIGN OO-->
            <Key
                latin:keySpec="&#x0C4B;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--s ే TELUGU VOWEL SIGN EE-->
            <Key
                latin:keySpec="&#x0C47;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--d ్ TELUGU SIGN VIRAMA-->
            <Key
                latin:keySpec="&#x0C4D;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--f ి TELUGU VOWEL SIGN I-->
            <Key
                latin:keySpec="&#x0C3F;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--g ు TELUGU VOWEL SIGN U-->
            <Key
                latin:keySpec="&#x0C41;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--h ప TELUGU LETTER PA-->
            <Key
                latin:keySpec="&#x0C2A;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--j ర TELUGU LETTER RA-->
            <Key
                latin:keySpec="&#x0C30;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--k క TELUGU LETTER KA-->
            <Key
                latin:keySpec="&#x0C15;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--l త TELUGU LETTER TA-->
            <Key
                latin:keySpec="&#x0C24;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!-- ; &#x0C1A; చTELUGU LETTER CA -->
            <Key
                latin:keySpec="&#x0C1A;"
                latin:moreKeys=";"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <!--' ట TELUGU LETTER TTA-->
            <Key
                latin:keySpec="&#x0C1F;"
                latin:moreKeys="'"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
         </default>
    </switch>
</merge>
