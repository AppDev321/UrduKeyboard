<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
 >
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
            <!--Q ਥ GURMUKHI LETTER THA-->
            <Key
                latin:keySpec="&#x0A25;"
                latin:keyLabelFlags="fontNormal" />

            <!--W ਾਂ  -->
            <Key
                latin:keySpec="&#x0A3E;"
                latin:keyLabelFlags="fontNormal" />

            <!--E ਓ GURMUKHI LETTER OO-->
            <Key
                latin:keySpec="&#x0A13;"
                latin:keyLabelFlags="fontNormal" />

            <!--R ੍ਰ  -->
            <Key
                latin:keySpec="&#x0A4D;"
                latin:keyLabelFlags="fontNormal" />

            <!--T ਠ GURMUKHI LETTER TTHA-->
            <Key
                latin:keySpec="&#x0A20;"
                latin:keyLabelFlags="fontNormal" />

            <!--Y ੈ GURMUKHI VOWEL SIGN AI-->
            <Key
                latin:keySpec="&#x0A48;"
                latin:keyLabelFlags="fontNormal" />

            <!--U ੂ GURMUKHI VOWEL SIGN UU-->
            <Key
                latin:keySpec="&#x0A42;"
                latin:keyLabelFlags="fontNormal" />

            <!--I ੀ GURMUKHI VOWEL SIGN II-->
            <Key
                latin:keySpec="&#x0A40;"
                latin:keyLabelFlags="fontNormal" />

            <!--O ੌ GURMUKHI VOWEL SIGN AU-->
            <Key
                latin:keySpec="&#x0A4C;"
                latin:keyLabelFlags="fontNormal" />

            <!--P ਫ GURMUKHI LETTER PHA-->
            <Key
                latin:keySpec="&#x0A2B;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <!--q ਤ GURMUKHI LETTER TA-->
            <Key
                latin:keySpec="&#x0A24;"
                latin:keyLabelFlags="fontNormal" />

            <!--w ਾ GURMUKHI VOWEL SIGN AA-->
            <Key
                latin:keySpec="&#x0A3E;"
                latin:keyLabelFlags="fontNormal" />

            <!--e ੲ GURMUKHI IRI-->
            <Key
                latin:keySpec="&#x0A72;"
                latin:keyLabelFlags="fontNormal" />

            <!--r ਰ GURMUKHI LETTER RA-->
            <Key
                latin:keySpec="&#x0A30;"
                latin:keyLabelFlags="fontNormal" />

            <!--t ਟ GURMUKHI LETTER TTA-->
            <Key
                latin:keySpec="&#x0A1F;"
                latin:keyLabelFlags="fontNormal" />

            <!--y ੇ GURMUKHI VOWEL SIGN EE-->
            <Key
                latin:keySpec="&#x0A47;"
                latin:keyLabelFlags="fontNormal" />

            <!--u ੁ GURMUKHI VOWEL SIGN U-->
            <Key
                latin:keySpec="&#x0A41;"
                latin:keyLabelFlags="fontNormal" />

            <!--i ਿ GURMUKHI VOWEL SIGN I-->
            <Key
                latin:keySpec="&#x0A3F;"
                latin:keyLabelFlags="fontNormal" />

            <!--o ੋ GURMUKHI VOWEL SIGN OO-->
            <Key
                latin:keySpec="&#x0A4B;"
                latin:keyLabelFlags="fontNormal" />

            <!--p ਪ GURMUKHI LETTER PA-->
            <Key
                latin:keySpec="&#x0A2A;"
                latin:keyLabelFlags="fontNormal" />
        </default>
    </switch>
</merge>
