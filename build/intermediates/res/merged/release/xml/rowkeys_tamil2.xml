                latin:keySpec="&#x0BBE;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0BA4;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0B9F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0BCD;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0B87;"
                latin:keyLabelFlags="fontNormal" />
         </default>
    </switch>
</merge>
