                latin:keySpec="&#x0BAA;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0BB4;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0BAE;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0B99;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0B83;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0B88;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <Key
                latin:keySpec="&#x0BAF;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0BB3;"
