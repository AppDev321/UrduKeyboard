<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
 >
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
            <Key
                latin:keySpec="&#x0C16;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C1B;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C20;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C25;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C2B;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C4D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C18;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x0C1D;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x0C22;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C27;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C2D;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <Key
                latin:keySpec="&#x0C15;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C1a;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C1F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C24;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C2A;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C4D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C17;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C1C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C21;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C26;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0C2C;"
                latin:keyLabelFlags="fontNormal" />
        </default>
    </switch>
</merge>
